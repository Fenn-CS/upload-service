{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint"],
  "extends": [
    "plugin:@typescript-eslint/all",
    "airbnb-typescript/base"
  ],
  "rules": {
    "@typescript-eslint/prefer-readonly-parameter-types": "off"
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  }
}
